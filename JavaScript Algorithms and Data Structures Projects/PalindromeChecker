function palindrome(str) {
    str = str.toUpperCase()
    console.log(str)
    console.log(Math.floor((str.length)/2))
    for(let i=0; i<Math.floor((str.length)/2); i++){
      console.log(str[i] +"-"+i)
      console.log(str[str.length-1-i] +`-${str.length-1-i}`)
      if(str[i] != str[str.length-1-i]){
        return false
      };
    };
    return true;
  };
  palindrome("eye");
  palindrome("nope")